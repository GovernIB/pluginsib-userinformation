#org.fundaciobit.sample.pluginsib.userinformation.keycloak.mapping.dir3Parent=dir3pare
#org.fundaciobit.sample.pluginsib.userinformation.keycloak.mapping.companyArea=areaNumber
#org.fundaciobit.sample.pluginsib.userinformation.keycloak.mapping.companyDepartment=departmentNumber
#org.fundaciobit.sample.pluginsib.userinformation.keycloak.mapping.surname1=sn1
#org.fundaciobit.sample.pluginsib.userinformation.keycloak.mapping.surname2=sn2


# Per depuració
org.fundaciobit.sample.pluginsib.userinformation.keycloak.debug=true

